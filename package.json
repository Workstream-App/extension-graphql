{
  "name": "@workstream-app/extension-graphql",
  "version": "0.1.0",
  "description": "hocuspocus webhook extension workstream.io",
  "homepage": "https://hocuspocus.dev",
  "keywords": [
    "workstream",
    "hocuspocus",
    "api",
    "graphql",
    "yjs"
  ],
  "license": "MIT",
  "type": "module",
  "main": "dist/workstream-graphql.cjs",
  "module": "dist/workstream-graphql.esm.js",
  "types": "dist/extension-graphql/src/index.d.ts",
  "exports": {
    "source": {
      "import": "./src"
    },
    "default": {
      "import": "./dist/workstream-webhook.esm.js",
      "require": "./dist/workstream-webhook.cjs"
    }
  },
  "files": [
    "src",
    "dist",
    "README.md"
  ],
  "scripts": {
    "testx": "ava",
    "test": "echo \"no test specified yet\"",
    "test:watch": "ava --watch",
    "build": "tsc --build tsconfig.json",
    "buildx": "tsc  ./src/index.ts --outDir ./dist --esModuleInterop",
    "clean": "rm -Rf ./dist",
    "release": "npm run lint && npm run test && npm run build:packages && lerna publish --no-private"
  },
  "dependencies": {
    "@hocuspocus/server": "^1.0.0-alpha.104",
    "@hocuspocus/transformer": "^1.0.0-alpha.23",
    "axios": "^0.26.1",
    "graphql": "^16.5.0",
    "graphql-ruby-client": "^1.11.1",
    "graphql-type-json": "^0.3.2",
    "yjs": "^13.5.29"
  },
  "devDependencies": {
    "@atomico/rollup-plugin-sizes": "^1.1.4",
    "@babel/core": "^7.17.8",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "concurrently": "^6.4.0",
    "eslint": "^8.11.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-import": "^2.25.4",
    "minimist": "^1.2.5",
    "rollup": "^2.70.1",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-typescript2": "^0.31.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.7.4"
  },
  "publishConfig": {
    "@workstream-app:registry": "https://npm.pkg.github.com"
  },
  "resolutions": {
    "prosemirror-view": ">=1.26.2"
  }
}
