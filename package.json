{
  "name": "@workstream-app/extension-graphql",
  "version": "0.2.5",
  "description": "hocuspocus webhook extension workstream.io",
  "homepage": "https://hocuspocus.dev",
  "keywords": [
    "workstream",
    "hocuspocus",
    "api",
    "graphql",
    "yjs"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Workstream-App/extension-graphql.git"
  },
  "license": "MIT",
  "type": "module",
  "main": "dist/extension-graphql.cjs",
  "module": "dist/extension-graphql.esm.js",
  "types": "dist/src/index.d.ts",
  "exports": {
    "source": {
      "import": "./src"
    },
    "default": {
      "import": "./dist/extension-graphql.esm.js",
      "require": "./dist/extension-graphql.cjs"
    }
  },
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "test": "ava",
    "test:watch": "ava --watch",
    "buildx": "tsc --build tsconfig.json",
    "build": "yarn version --patch && rollup -c",
    "clean": "rm -Rf ./dist",
    "release": "npm run lint && npm run test && npm run build && lerna publish --no-private"
  },
  "dependencies": {
    "@hocuspocus/server": "^1.0.0-alpha.104",
    "@hocuspocus/transformer": "^1.0.0-alpha.23",
    "@types/node": "^18.6.3",
    "axios": "^0.27.2",
    "eslint-config-google": "^0.14.0",
    "yjs": "^13.5.41"
  },
  "devDependencies": {
    "@atomico/rollup-plugin-sizes": "^1.1.4",
    "@ava/typescript": "^3.0.1",
    "@babel/core": "^7.18.10",
    "@babel/eslint-parser": "^7.18.9",
    "@babel/preset-env": "^7.18.10",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "ava": "^4.3.1",
    "concurrently": "^7.3.0",
    "eslint": "^8.21.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-import": "^2.26.0",
    "minimist": "^1.2.6",
    "rollup": "^2.77.2",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-typescript2": "^0.32.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "resolutions": {
    "prosemirror-view": "^1.27.0",
    "yjs": "^13.5.41"
  }
}
